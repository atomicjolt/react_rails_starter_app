{
  "name": "react_rails_starter_app",
  "version": "1.0.0",
  "description": "React Starter App with Rails. Uses webpacker.",
  "author": "atomicjolt",
  "license": "MIT",
  "homepage": "https://github.com/atomicjolt/react_rails_starter_app",
  "scripts": {
    "test": "jest --config package.json",
    "test_update": "jest -u --config package.json",
    "test_debug": "node inspect jest --runInBand --config package.json || SUCCESS=false",
    "hot": "./bin/webpack-dev-server",
    "lint": "eslint client",
    "lint_fix": "eslint client --fix",
    "nuke": "rm -rf node_modules",
    "inspect_fuel": "\"${EDITOR:-vi}\" ./node_modules/atomic-fuel"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/atomicjolt/react_rails_starter_app"
  },
  "keywords": [
    "React",
    "Rails",
    "Atomic Jolt"
  ],
  "jest": {
    "roots": [
      "<rootDir>/client"
    ],
    "verbose": true,
    "moduleDirectories": [
      "node_modules",
      "<rootDir>/client"
    ],
    "transform": {
      "\\.(gql|graphql)$": "jest-transform-graphql",
      ".*": "babel-jest"
    },
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "./client/testing/__mocks__/file_mock.js",
      "\\.(sass|scss|css|less)$": "./client/testing/__mocks__/style_mock.js"
    },
    "setupFilesAfterEnv": [
      "./client/testing/shim.js",
      "./client/testing/setup_tests.js"
    ]
  },
  "dependencies": {
    "@apollo/react-hooks": "^3.1.3",
    "@apollo/react-testing": "^3.1.3",
    "@babel/core": "^7.7.7",
    "@babel/plugin-proposal-class-properties": "^7.7.4",
    "@babel/plugin-proposal-object-rest-spread": "^7.7.7",
    "@babel/plugin-syntax-dynamic-import": "^7.7.4",
    "@babel/plugin-transform-destructuring": "^7.7.4",
    "@babel/plugin-transform-regenerator": "^7.7.5",
    "@babel/plugin-transform-runtime": "^7.7.6",
    "@babel/preset-env": "^7.7.7",
    "@babel/preset-react": "^7.7.4",
    "@rails/webpacker": "^4.2.2",
    "apollo-cache-inmemory": "^1.6.5",
    "apollo-client": "^2.6.8",
    "apollo-link": "^1.2.13",
    "apollo-link-http": "^1.5.16",
    "apollo-link-state": "^0.4.0",
    "atomic-fuel": "^4.2.1",
    "babel-loader": "^8.0.6",
    "babel-plugin-dynamic-import-node": "^2.3.0",
    "babel-plugin-macros": "^2.8.0",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.24",
    "babel-preset-es2015": "^6.24.1",
    "core-js": "^3.6.2",
    "css-loader": "^2.1.1",
    "es6-promise": "^4.2.4",
    "file-loader": "^4.3.0",
    "graphql": "^14.5.8",
    "graphql-tag": "^2.5.0",
    "history": "^4.10.1",
    "lodash": "4.17.15",
    "mime": "^2.4.4",
    "prop-types": "^15.7.2",
    "react": "^16.12.0",
    "react-apollo": "^3.1.3",
    "react-dom": "^16.12.0",
    "react-proxy-loader": "^0.3.5",
    "react-redux": "^7.1.3",
    "react-router": "^5.1.2",
    "react-router-dom": "^5.1.2",
    "react-router3": "npm:react-router@3.2.3",
    "redux": "4.0.5",
    "regenerator-runtime": "^0.13.3",
    "sass-loader": "^8.0.0",
    "superagent": "^5.1.2",
    "uuid": "^3.3.3",
    "webpack": "^4.41.5",
    "webpack-cli": "^3.3.10"
  },
  "devDependencies": {
    "babel-eslint": "^10.0.3",
    "babel-jest": "^24.9.0",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "eslint": "^6.7.2",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-plugin-babel": "^5.3.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.17.0",
    "eslint-plugin-react-hooks": "^2.3.0",
    "eventsource-polyfill": "^0.9.6",
    "jest": "^24.9.0",
    "jest-transform-graphql": "^2.1.0",
    "nock": "^11.7.2",
    "react-test-renderer": "^16.12.0",
    "wait-for-expect": "^3.0.1",
    "webpack-dev-server": "^3.9.0"
  }
}
